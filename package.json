{
    "name": "kommune-bot",
    "description": " ",
    "version": "0.1.1",
    "main": "main.js",
    "scripts": {
        "start": "node main.js",
        "watch:server": "cd server && tsc --watch",
        "watch:client": "cd client && tsc --watch",
        "build": "npm run build:server && npm run build:client",
        "build:server": "cd server && tsc",
        "build:client": "cd client && tsc",
        "install:server": "cd server && npm install",
        "install:client": "cd client && npm install",
        "install:full": "npm run install:server && npm run install:client && npm install"
    },
    "dependencies": {
        "dotenv": "^4.0.0"
    }
}